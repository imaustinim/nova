{
  "name": "MERN Boilerplate",
  "version": "0.0.1",
  "public": true,
  "scripts": {
    "start": "node ./bin/www",
    "start-dev": "nodemon server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix ../client",
